<DOCTYPE>
<html>
<head>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<script>
$(document).ready(function() {
   for (var i = 0; i < 768; i++) {
   $(".wrapper").append("<div></div>");
   $("div").children().addClass("inner");
   } 
   $(".inner").mouseenter(function(){
   	$(this).fadeTo("slow", 0);
   });
});
</script>
<style>
.wrapper {
	width: 960px;
	height: 600px;
	background: url("http://info.searchlabmarketing.com/hs-fs/hub/15687/file-1101596526-jpg/rainbow-background.jpg-1");
	background-position: 50% 50%;
	margin: 0 auto;
}
.inner {
	display: inline-block;
	height: 25px;
	width: 30px;
	background: black;
}
</style>
</head>
<body>
<div class="wrapper">
</div>
</body>
